    "url": "git+https://github.com/conorhastings/react-emoji-react.git"
  },
  "author": "Conor Hastings",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conorhastings/react-emoji-react/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13"
  },
  "peerDependencies": {
    "react": ">= 0.14.0",
    "react-dom": ">= 0.14.0"
  },
  "dependencies": {
    "get-emoji": "^2.0.0"
